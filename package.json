{
  "name": "astrotour",
  "version": "1.0.0",
  "description": "Astrotour Webpage",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefixer:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefixer:css compress:css"
  },
  "author": "Aravinth",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "concat": "^1.0.3",
    "gulp": "^4.0.2",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "postcss": "^8.3.5"
  }
}
